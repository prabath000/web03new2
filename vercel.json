{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "$1",
      "headers": {
        "X-Robots-Tag": "noindex"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
